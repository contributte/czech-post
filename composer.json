{
  "name": "contributte/czech-post",
  "description": "Czech Post API client",
  "keywords": [
    "nette",
    "czechpost",
    "api",
    "client"
  ],
  "type": "library",
  "license": "MPL-2.0",
  "homepage": "https://github.com/contributte/czech-post",
  "authors": [
    {
      "name": "Tomáš Sedláček",
      "homepage": "https://github.com/kedlas"
    },
    {
      "name": "Marek Bartoš",
      "homepage": "https://github.com/mabar"
    }
  ],
  "require": {
    "php": ">=7.2",
    "ext-simplexml": "*",
    "guzzlehttp/guzzle": "^6.3.3",
    "nette/di": "~3.0.0",
    "nette/utils": "~3.1.0"
  },
  "require-dev": {
    "ninjify/qa": "^0.11.0",
    "phpstan/extension-installer": "^1.0",
    "phpstan/phpstan": "^0.12.0",
    "phpstan/phpstan-deprecation-rules": "^0.12.0",
    "phpstan/phpstan-nette": "^0.12.0",
    "phpstan/phpstan-strict-rules": "^0.12.0",
    "phpunit/phpunit": "^8.0.0"
  },
  "autoload": {
    "psr-4": {
      "Contributte\\CzechPost\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Cases\\Contributte\\CzechPost\\": "tests/cases",
      "Tests\\Toolkit\\Contributte\\CzechPost\\": "tests/toolkit"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.2.x-dev"
    }
  }
}
