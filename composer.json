{
  "name": "contributte/czech-post",
  "description": "Czech Post API client",
  "keywords": [
    "nette",
    "czechpost",
    "api",
    "client"
  ],
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/contributte/czech-post",
  "authors": [
    {
      "name": "Milan Felix Å ulc",
      "homepage": "https://f3l1x.io"
    }
  ],
  "require": {
    "php": ">=8.1",
    "ext-simplexml": "*",
    "ext-libxml": "*",
    "ext-json": "*",
    "guzzlehttp/guzzle": "^7.4.0",
    "nette/di": "^3.1.8",
    "nette/utils": "^4.0.3"
  },
  "require-dev": {
    "contributte/qa": "^0.4",
    "contributte/tester": "^0.4",
    "contributte/phpstan": "^0.1"
  },
  "autoload": {
    "psr-4": {
      "Contributte\\CzechPost\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.4.x-dev"
    }
  }
}
